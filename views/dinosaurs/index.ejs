<h1>List of Dinosaurs</h1>

<form action='/dinosaurs' method='GET'>
    <label for='nameFilter'>Filter by Name:</label>
    <input type='text' id='nameFilter' name='nameFilter'>
    <input type='submit'>
</form>

<ul>
    <% dinosaurs.forEach((dino)=>{ %>
        <li> <%=dino.name%> is a(n) <%=dino.type%> </li>
    <% }) %>
</ul>